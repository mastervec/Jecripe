var ChorandoAudio:AudioClip;
var BeijoAudio:AudioClip;
var ContrariedadeAudio:AudioClip;
var LambendoAudio:AudioClip;
var PalmasAudio:AudioClip;
var BalbuciandoAudio:AudioClip;

function ChangeToChorando()
{
	GetComponent.<AudioSource>().clip = ChorandoAudio;
	GetComponent.<AudioSource>().Play();
}

function ChangeToBeijo()
{
	GetComponent.<AudioSource>().clip = BeijoAudio;
	GetComponent.<AudioSource>().Play();
}

